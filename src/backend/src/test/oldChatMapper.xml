<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backend.oldchat.ChatMapper">
    <insert id="save" parameterType="com.example.backend.oldchat.ChatMessage">
        INSERT INTO chat (content,sessionid)
        VALUES (#{content},#{sessionid})
    </insert>
</mapper>

